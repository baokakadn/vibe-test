<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.axonvibe.challenge.mobile">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="false"
        android:label="@string/all_app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:allowTaskReparenting="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".main.view.AnswerIncorrectActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".main.view.AnswerCorrectActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".main.view.ScanQRActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".main.view.AnswerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".main.view.WaitingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".main.view.SuccessAuthActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".main.view.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".main.view.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>